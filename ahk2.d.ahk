 *     EnumWindowsProc(hwnd, lParam){
 *         win_title := WinGetTitle(hwnd)
 *         if win_title
 *             Output .= 'HWND: ' hwnd
 *                 . '`tTitle: ' win_title
 *                 . '`tClass: ' WinGetClass(hwnd) '`n'
 *         return true
 *     }
 * }
